<div id="header">
  <%= link_to 'New Listing', new_property_path, class: 'button' %>
  <%= link_to 'All Listings', properties_path, class: 'button' %>
  <%= link_to 'Rentals', rentals_properties_path, class: 'button' %>
  <%= link_to 'For Sale', for_sale_properties_path, class: 'button' %>
  <%= link_to 'Houses', houses_properties_path, class: 'button' %>
  <%= link_to 'Condos', condos_properties_path, class: 'button' %>
  <%= link_to 'Apartments', apartments_properties_path, class: 'button' %>
  <%= link_to 'Recently Listed', recent_properties_path, class: 'button' %>
</div>

<div id="flash">
  <% flash.each do |name, msg| %>
    <% if msg.is_a?(String) %>
      <div class="alert alert-<%= name == :notice ? "success" : "error" %>">
        <%= msg %>
      </div>
    <% end %>
  <% end %>
</div>